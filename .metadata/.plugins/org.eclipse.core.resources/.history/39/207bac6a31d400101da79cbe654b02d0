spring:
  application:
    name: tms-backend

  datasource:
    url: jdbc:postgresql://localhost:5432/tms_server
    username: postgres
    password: root
    driver-class-name: org.postgresql.Driver

  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
